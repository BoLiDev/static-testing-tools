  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "format": "prettier --ignore-path .gitignore --write \"*/**.+(js|jsx|json|ts|tsx|html|css|sass|scss)\"",
    "build": "babel src --out-dir dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8"
  }
}
